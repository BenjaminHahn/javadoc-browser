<?xml version="1.0" encoding="UTF-8"?>


<?import de.hahn.apibrowser.views.ApiAccordion?>
<?import de.hahn.apibrowser.views.EngineView?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.VBox?>
<VBox prefHeight="720.0" prefWidth="1280.0" fx:id="mainWindow"
      xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="de.hahn.apibrowser.Controller">
    <children>
        <SplitPane dividerPositions="0.15,0.9" VBox.vgrow="ALWAYS">
            <items>
                <ApiAccordion fx:id="apiAccordion" minWidth="0"/>
                <EngineView fx:id="engineView" tabClosingPolicy="ALL_TABS"/>
            </items>
        </SplitPane>
    </children>
</VBox>
